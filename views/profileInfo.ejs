  <body>
    <header class="container">
  
        <h1 class=""><a href="/profile">P4P</a></h1>
      
      
        <a href="/signUp" class="loginBtn"> SignUp</a>
     
      
    </header>

<main class="container">
  <div class="cardContainer">
    <section class="loginCard">
    
      <form class="loginForm" action="/postProfileInfo" enctype="multipart/form-data" method="POST">
        <label for="imageUpload">profile picture</label>
        <input type="file" class="form-control" id="imageUpload" name="file" size="100">
         
          <input
            type="text"
            class="form-control"
            id="skills"
            aria-describedby="emailHelp"
            name="skills"
            placeholder="design, development, engineering ..."
          />

          <input
            type="text"
            class="form-control"
            id="interests"
            name="interests"
            placeholder="tech, health, art ...."
          />
          <input type="submit" class="loginpageBtn" name="" id="">
        </form>
    </section>
  </div>
</main>

